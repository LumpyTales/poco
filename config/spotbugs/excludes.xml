<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter
        xmlns="https://github.com/spotbugs/filter/3.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">

    <Match>
        <!-- This rule EI_EXPOSE_REP forces, together with EI_EXPOSE_REP2, to implement duplicate code -->
        <Bug code="EI" />
    </Match>
    <Match>
        <!-- This rule EI_EXPOSE_REP forces, together with EI_EXPOSE_REP2, to implement duplicate code -->
        <Bug code="EI2" />
    </Match>
    <Match>
        <!-- This rule CT_CONSTRUCTOR_THROW is not necessary -->
        <Bug code="CT" />
        <Class name="io.github.lumpytales.poco.core.CollectorGeneratorParams" />
    </Match>
</FindBugsFilter>